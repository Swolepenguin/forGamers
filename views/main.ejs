
<div id="SFGH">
  <h1>
    Search For Games Here
  </h1>
</div>

<div id="searchbar">
  <form action="/games" method="GET" >
    <input type="text" placeholder="search" name="game" id="game"></input>
    <input type="submit"> 
    <script>debugger</script>
  </form>
</div>
 

<div id="new"> Top Rated </div>
<div class="cp">
<ul>
  <% newGames.forEach(gm => { debugger %>
<img class='gameimg' src="<%= gm.background_image %> "> </img>       
<%  }) %>
</ul>
</div>
<div class="displayT">
  <ul>
    <% newGames.forEach(gm => { %>
      <h3> Title: <%= gm.name  %> </h3> 
    <% }) %> 
  </ul>
</div>

<div id="new"> New Releases </div>
<div class="cp">
<ul>
  <% topRated.forEach(gm => { debugger %>
<img class='gameimg' src="<%= gm.background_image %> "> </img>       
<%  }) %>
</ul>
</div>
<div class="displayT">
  <ul>
    <% topRated.forEach(gm => { %>
      <h3> Title: <%= gm.name  %> </h3> 
    <% }) %> 
  </ul>
</div>

