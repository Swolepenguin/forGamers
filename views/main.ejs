
<div id="SFGH">
  <h1>
    Search For Games Here
  </h1>
</div>

<div id="searchbar">
  <form action="/games" method="GET" >
    <input type="text" placeholder="search" name="game" id="game"></input>
    <input type="submit"> 
    <!-- <script>debugger</script> -->
  </form>
</div>
 

<div id="new"> Top Rated </div>
<div class="cp">
<ul>
  <% newGames.forEach(gm => { debugger %>
    <div class="card" style="width: 25rem;">
      <img src="<%= gm.background_image %> " class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title"><%= gm.name  %> </h5>
        <p class="card-text"><%= gm.description  %> </p>
       
      </div>
    </div>
    <% }) %> 
  </ul>
</div>


<div id="new"> New Releases </div>
<div class="cp">
<ul>
  <% topRated.forEach(gm => { debugger %>
    <div class="card" style="width: 25rem;">
      <img src="<%= gm.background_image %> " class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title"><%= gm.name  %> </h5>
        <p class="card-text"></p>

      </div>
    </div>
    <% }) %> 
  </ul>
</div>

